{
  "java.configuration.updateBuildConfiguration": "disabled",
  "java.import.gradle.enabled": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "bin/": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true,
    "**/*~": true
  },
  "C_Cpp.default.configurationProvider": "vscode-wpilib",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#d1ca28",
    "activityBar.background": "#d1ca28",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#1a8681",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#d1ca28",
    "statusBar.background": "#a6a120",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#7b7818",
    "statusBarItem.remoteBackground": "#a6a120",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#a6a120",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#a6a12099",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#a6a120"
}
